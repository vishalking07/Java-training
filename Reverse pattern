import java.util.*;
public class Main
{
	public static void main(String[] args) {
	Scanner sc = new Scanner (System.in);
	int n=sc.nextInt();
	int sum=0,temp=0,nevv=temp;
	for(int i=1 ; i<=n ;i++){
	    for (int j =0 ;j<i;j++){
	        if (j==0){
	            temp=temp+i;
	           nevv=temp;
	            System.out.print(temp" ");
	        }
	       else{
	           nevv--;
	           System.out.print(nevv+" ");
	       }
	}
	System.out.printf("\n");
	}
}}
	   
