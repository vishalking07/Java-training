import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		int reverse=0,digit,reversed,num;
		num =n;
		while (n>0)
		{
		    digit=n%10;
		    reverse= (reverse*10)+digit;
		    n=n/10;
		    
		}
		if (num == reverse){
		    System.out.print("It was palindome");
		}
		else{
		    System.out.print("Its not a palindome");
		}
	}
}
