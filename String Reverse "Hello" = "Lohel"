import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner (System.in);
		String n = sc.nextLine();
		String result;
		int temp=n.length()-2;
		for (int i=0;i<2;i++){
		    
		System.out.print(n.charAt(temp));
		temp++;
		}
		for(int j=0;j<n.length()-2;j++){
		  System.out.print(n.charAt(j));  
		}
	}
}
