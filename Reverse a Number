import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n=sc.nextInt();
		int reverse=0,digit,reversed;
		while (n>0)
		{
		    digit=n%10;
		    reverse= (reverse*10)+digit;
		    n=n/10;
		    
		}
		System.out.print(reverse);
	}
}
